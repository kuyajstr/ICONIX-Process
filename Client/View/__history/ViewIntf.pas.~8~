unit ViewIntf;

interface

uses
  PresenterIntf;

type
  IView= interface
    ['{24CA421D-C6BA-4DFD-AE63-127ABFF1EC8D}']
    function GetController: IPresenter;
    procedure SetController(AController: IPresenter);

    property Controller: IPresenter read GetController write SetController;
  end;

implementation

end.
